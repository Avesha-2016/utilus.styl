$arr = a b c

$a = utilus_collect_by_type(10px #FFF utilus_get false 'adssad' {} asdfsadf true (sss rrr) red rgba false {a: b, c: d} 0 moo 'asaa' 45em '' aaaaaa $arr)

.foo
  strings: $a.strings
  idents: $a.idents
  units: $a.units
  rgbas: $a.rgbas
  colors: $a.colors
  functions: $a.functions
  objects: $a.objects
  booleans: $a.booleans
  arrays: $a.arrays

.foo_first
